{
  "name": "@repo/api",
  "version": "0.0.0-develop",
  "private": true,
  "type": "module",
  "imports": {
    "#*": "./src/*"
  },
  "exports": {
    ".": "./src/routes/router.ts",
    "./configuration": "./src/configuration.ts"
  },
  "scripts": {
    "start": "node --experimental-strip-types --env-file=.env ./src/server.ts",
    "dev": "node --experimental-strip-types --env-file=.env --watch --watch-preserve-output ./src/server.ts",
    "lint:eslint": "eslint . --max-warnings 0",
    "lint:typescript": "tsc --noEmit"
  },
  "dependencies": {
    "@repo/utils": "workspace:*",
    "@repo/models": "workspace:*",
    "@trpc/server": "catalog:",
    "zod": "catalog:",
    "fastify": "catalog:",
    "@fastify/cors": "catalog:",
    "@fastify/helmet": "catalog:",
    "@fastify/rate-limit": "catalog:",
    "@fastify/sensible": "catalog:",
    "superjson": "catalog:",
    "kysely": "catalog:",
    "pretty-ms": "catalog:"
  },
  "devDependencies": {
    "@repo/config-typescript": "workspace:*",
    "@repo/config-eslint": "workspace:*",
    "@total-typescript/ts-reset": "catalog:",
    "@types/node": "catalog:",
    "typescript-eslint": "catalog:",
    "eslint": "catalog:",
    "typescript": "catalog:"
  }
}
